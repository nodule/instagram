{
  "name": "tagSearch",
  "ns": "instagram",
  "description": "Search for matching tags by name",
  "phrases": {
    "active": "Searching tags like {{input.tag}}"
  },
  "ports": {
    "input": {
      "instagram": {
        "title": "InstagramID",
        "type": "InstagramAPI",
        "required": true
      },
      "tag": {
        "title": "Tag",
        "type": "string",
        "required": true
      }
    },
    "output": {
      "error": {
        "title": "Error",
        "type": "object"
      },
      "out": {
        "title": "Data",
        "type": "object",
        "properties": {
          "data": {
            "title": "Pagination",
            "type": "object"
          },
          "pagination": {
            "title": "Pagination",
            "type": "object"
          }
        }
      }
    }
  }
}
